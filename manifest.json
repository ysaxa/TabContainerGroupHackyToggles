{
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
      "scripts": ["background.js"]
  },
  "description": "An extension that adds hacky tabs to turn containers into Chrome-like tab groups.",
  "homepage_url": "https://github.com/ysaxa/TabContainerGroupHackyToggles",
  "manifest_version": 2,
  "name": "Tab Container Hacky Toggles",
  "permissions": [
    "tabs", "tabHide", "contextualIdentities", "cookies"
  ],
  "version": "1.0"
}
